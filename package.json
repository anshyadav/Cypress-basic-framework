  "bugs": {
    "url": "https://github.com/anshyadav/Cypress-basic-framework/issues"
  },
  "homepage": "https://github.com/anshyadav/Cypress-basic-framework#readme",
  "dependencies": {
    "cypress": "^6.6.0",
    "mailhog": "^4.15.0"
  },
  "devDependencies": {
    "cypress-mailosaur": "^2.1.2"
  }
}
